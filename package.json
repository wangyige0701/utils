{
	"name": "@wang-yige/utils",
	"version": "0.0.17",
	"type": "module",
	"description": "Some utils for JavaScript / TypeScript, collected from daily use",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		}
	},
	"sideEffects": false,
	"files": [
		"dist"
	],
	"keywords": [
		"utils",
		"JavaScript",
		"TypeScript"
	],
	"author": "Yige Wang <wyg1776686515@gmail.com>",
	"homepage": "https://github.com/wangyige0701/utils#readme",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wangyige0701/utils.git"
	},
	"scripts": {
		"build": "rollup -c",
		"browser": "rollup -c -w --context=browser",
		"typecheck": "tsc --noEmit",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"publish": "npm publish --access=public"
	},
	"devDependencies": {
		"@rollup/plugin-alias": "^5.1.0",
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@types/node": "^22.4.2",
		"@vitest/coverage-istanbul": "^2.0.5",
		"prettier": "^3.3.3",
		"rollup": "^4.21.0",
		"rollup-plugin-delete": "^2.1.0",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-esbuild": "^6.1.1",
		"rollup-plugin-typescript2": "^0.36.0",
		"ts-node": "^10.9.2",
		"tslib": "^2.7.0",
		"typescript": "^5.5.4",
		"vitest": "^2.0.5"
	},
	"packageManager": "pnpm@9.1.0"
}
